    <servlet-name>EditBookController</servlet-name>
    <url-pattern>/editBook</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditBookSaveController</servlet-name>
    <url-pattern>/editBookSave</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchBooksController</servlet-name>
    <url-pattern>/searchBooks</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyAccountController</servlet-name>
    <url-pattern>/myAccount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditPasswordController</servlet-name>
    <url-pattern>/editPassword</url-pattern>
  </servlet-mapping>
</web-app>